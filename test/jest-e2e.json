{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "setupFilesAfterEnv": [
    "<rootDir>/jest.setup.ts"
  ],
  "testEnvironment": "node",
  "testPathIgnorePatterns": [
    "node_modules"
  ],
  "testRegex": ".spec.ts$",
  "transform": {
    "^.+\\.ts$": "ts-jest"
  }
}